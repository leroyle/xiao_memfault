
{
    "name": "Memfault SDK",
    "version": "1.0",
    "keywords": "Memfault",
    "description": "Memfault SDK",
    "repository": {
        "type": "git",
        "url": "https://github.com/noidea/noidea.git"
    },
    "frameworks": "arduino",
    "platforms": "nordicnrf52",
    "build": {
	"includeDir": ".",
        "libArchive": false,
		"flags": [
			" -D MEMFAULT_SDK_INCS",
			" -D MEMFAULT_LIB",
			"-Iinclude",
			"-I components/include",
			"-I components/include/memfault",
			"-I ports/include",
			"-I examples/nrf5/apps/memfault_demo_app/third_party/memfault",
			"-I ../nrf5_sdk/components/libraries/balloc",
			"-I ../nrf5_sdk/components/libraries/cli",
			"-I ../nrf5_sdk/components/libraries/cli/rtt",
			"-I ../nrf5_sdk/components/libraries/delay",
			"-I ../nrf5_sdk/components/libraries/log/src",
			"-I ../nrf5_sdk/components/libraries/memobj",
			"-I ../nrf5_sdk/components/libraries/pwr_mgmt",
			"-I ../nrf5_sdk/components/libraries/experimental_section_vars",
			"-I ../nrf5_sdk/components/libraries/log",
			"-I ../nrf5_sdk/components/libraries/mutex",
			"-I ../nrf5_sdk/components/libraries/stack_guard/",
			"-I ../nrf5_sdk/components/libraries/strerror",
			"-I ../nrf5_sdk/components/libraries/timer",
			"-I ../nrf5_sdk/components/libraries/queue",
			"-I ../nrf5_sdk/components/libraries/ringbuf",
			"-I ../nrf5_sdk/components/libraries/util",
			"-I ../nrf5_sdk/components/softdevice/common/",
			"-I ../nrf5_sdk/components/softdevice/s140/headers/",
			"-I ../nrf5_sdk/modules/nrfx/templates",
			"-I ../nrf5_sdk/modules/nrfx/templates/nRF52840",
			"-I ../nrf5_sdk/components/toolchain/cmsis/include",
			"-I ../nrf5_sdk/external/fprintf/",
			"-I ../nrf5_sdk/external/freertos/config/",
			"-I ../nrf5_sdk/external/freertos/source/include",
			"-I ../nrf5_sdk/external/freertos/portable/GCC/nrf52",
			"-I ../nrf5_sdk/external/freertos/portable/CMSIS/nrf52",
			"-I ../nrf5_sdk/integration/nrfx/legacy",
			"-I ../nrf5_sdk/modules/nrfx",
			"-I ../nrf5_sdk/modules/nrfx/hal",
			"-I ../nrf5_sdk/modules/nrfx/drivers",
			"-I ../nrf5_sdk/modules/nrfx/drivers/include",
			"-I ../nrf5_sdk/modules/nrfx/mdk",
			"-I FreeRTOS",
			"-I Source/LAL"

		],
      "extraScript": "extra_script.py"
    }
}

